{
  "manifest_version": 2,

  "name": "Last Tab",
  "description": "Some description.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "commands": {
    "toggle_last_tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+P",
        "mac": "Command+Shift+P"
      },
      "description": "Toggle between the previous and current tabs.",
      "global": false
    }
  }
}
